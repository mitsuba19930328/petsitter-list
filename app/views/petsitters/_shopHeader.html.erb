      </div>
      <div class='shop-top-info_reviews'>
        <%= link_to root_path do %>
          <i class='fas fa-comment-dots'></i>2 件
        <% end %>
      </div>
      <div class='shop-top-info_bookmarks'>
        <%= link_to root_path do %>
          <i class='fas fa-bookmark'></i>4 件
        <% end %>
      </div>
    </div>
    <div class="shop-bottom-info">
      <span>エリア：</span>
      <%= link_to "中野", root_path, class: 'shop-bottom-info-show' %>
      <span>対応可能動物種類：</span>
      <div class='pet-types'>
        <ul class='pet-types-list'>
          <%= link_to "犬", root_path, class:'shop-bottom-info-show' %>
          <%= link_to "猫", root_path, class:'shop-bottom-info-show' %>
        </ul>
      </div>
    </div>
  </div>

  <div class='action-wrap'>
    <div class='action-wrap_app'>
      <div class='action-btn' id='testsample'>
        <%= link_to root_path, class:'action-link' do %>
          <i class='fas fa-user-check fa-lg'></i> <span class='action-name'>行った</span>
        <% end %>
