
<!-- ペットシッター詳細の上部部分 -->
<div class="breadcrumbs-list">ペットシッターリスト › 全てのペットシッター一覧 › たんぽぽハウス 高田馬場店</div>
<div class="shop-header">
  <div class="info-wrap">
    <div class="shop-controller">
      <div class="shop-name">
        <%= @petsitter.name %>
      </div>
    </div>
    <div class="shop-top-info">
      <div class="shop-top-info_rate">
        <!--星評価-->
        <div id="star-rate-<%= @petsitter.id %>"></div>
        <script>
            $('#star-rate-<%= @petsitter.id %>').raty({
                size: 36,
                starOff:  '<%= asset_path('star-off.png') %>',
                starOn : '<%= asset_path('star-on.png') %>',
                starHalf: '<%= asset_path('star-half.png') %>',
                // half: true,
                readOnly: true,
                score: "<%= @reviews.average(:rate).to_f.round(2) %>",
            });
        </script>

        <%= @reviews.average(:rate).to_f.round(2) %>
        <!--/星評価-->


